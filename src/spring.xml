<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
                  "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

                  
<beans>
	<bean id="Triangle" class="com.demo.Triangle" >
		<property name="a" ref="zeroPoint"></property>
		<property name="b" ref="onePoint"></property>
		<property name="c" ref="twoPoint"></property>
	</bean>
	<bean id="zeroPoint" class="com.demo.Point">
		<property name="x" value="0"></property>
		<property name="y" value="0"></property>
	</bean>
	<bean id="onePoint" class="com.demo.Point">
		<property name="x" value="-20"></property>
		<property name="y" value="0"></property>
	</bean>
	<bean id="twoPoint" class="com.demo.Point">
		<property name="x" value="0"></property>
		<property name="y" value="20"></property>
	</bean>
</beans>